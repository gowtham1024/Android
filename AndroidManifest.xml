            <intent-filter>
                <action android:name="com.streak.test1.MENU" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.streak.test1.Menu2"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.streak.test1.MENU2" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.streak.test1.Textplay"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.streak.test1.TEXTPLAY" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.streak.test1.Email"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.streak.test1.EMAIL" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
