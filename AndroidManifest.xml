            android:name="com.streak.test1.Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
         <activity
            android:name="com.streak.test1.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.streak.test1.MAINACTIVITY" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.streak.test1.Scren"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.streak.test1.SCREN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.streak.test1.Menu"
